{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3} {SourceContext} TraceId:{ActivityTraceId}] {Message:lj} {NewLine}{Exception} {PlatformBackgroundThreadFullStackTrace}"
        }
      }
    ]
  },
  "SeedDummyTestPerformance": false
}
